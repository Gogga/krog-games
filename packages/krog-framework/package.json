{
  "name": "@krog/krog-framework",
  "version": "1.0.0",
  "description": "KROG mathematical framework for formal game rule specifications",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "ts-node src/KROG-TESTS.ts"
  },
  "dependencies": {
    "@krog/types": "workspace:*",
    "chess.js": "^1.4.0"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "tsup": "^8.0.0",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
